<Project>
    <PropertyGroup>
        <Version>1.0.0</Version>
        <TargetFrameworks>net6;net7</TargetFrameworks>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
        <Nullable>enable</Nullable>
        <PackageDescription>This package provides functionalities decoupled access control</PackageDescription>
        <RepositoryUrl>https://github.com/f-waffenschmidt/Authorization</RepositoryUrl>
        <Authors>Florian Waffenschmidt</Authors>
    </PropertyGroup>
</Project>
